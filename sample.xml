<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>Validate the latest deployed version to PROD environment.</description>
  <keepDependencies>true</keepDependencies>
  <properties>
    <jenkins.model.BuildDiscarderProperty>
      <strategy class="hudson.tasks.LogRotator">
        <daysToKeep>-1</daysToKeep>
        <numToKeep>5</numToKeep>
        <artifactDaysToKeep>-1</artifactDaysToKeep>
        <artifactNumToKeep>-1</artifactNumToKeep>
      </strategy>
    </jenkins.model.BuildDiscarderProperty>
    <hudson.plugins.disk__usage.DiskUsageProperty plugin="disk-usage@0.28"/>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.StringParameterDefinition>
          <name>IGNORED_FLOWS</name>
          <description>Comma separated name of flows to be ignored in this deployment </description>
          <defaultValue>SetAccountOnCase-2,Create_Closed_Sales_Case-4,U2C_MasterCode-4</defaultValue>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.plugins.git.GitSCM" plugin="git@3.0.1">
    <configVersion>2</configVersion>
    <userRemoteConfigs>
      <hudson.plugins.git.UserRemoteConfig>
        <url>https://rawdee@bitbucket.org/ehsan_mahmoudi_acn/p2a.git</url>
        <credentialsId>jenkins_bitbucket</credentialsId>
      </hudson.plugins.git.UserRemoteConfig>
    </userRemoteConfigs>
    <branches>
      <hudson.plugins.git.BranchSpec>
        <name>P2A-PROD-R1.A-HotFix-W17</name>
      </hudson.plugins.git.BranchSpec>
    </branches>
    <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
    <browser class="hudson.plugins.git.browser.BitbucketWeb">
      <url>https://bitbucket.org/ehsan_mahmoudi_acn/p2a</url>
    </browser>
    <submoduleCfg class="list"/>
    <extensions/>
  </scm>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Ant plugin="ant@1.2">
      <targets>component-validate</targets>
      <antName>Ant-1.9.7</antName>
      <properties>salesforce.serverUrl=login.salesforce.com
env=prod
admin.username=${sf.username}
admin.password=${sf.password}
ignoreFlows=${IGNORED_FLOWS}</properties>
    </hudson.tasks.Ant>
  </builders>
  <publishers>
    <hudson.plugins.git.GitPublisher plugin="git@3.0.1">
      <configVersion>2</configVersion>
      <pushMerge>true</pushMerge>
      <pushOnlyIfSuccess>true</pushOnlyIfSuccess>
      <forcePush>false</forcePush>
    </hudson.plugins.git.GitPublisher>
    <hudson.tasks.junit.JUnitResultArchiver plugin="junit@1.19">
      <testResults>reports/junit/*.xml</testResults>
      <keepLongStdio>false</keepLongStdio>
      <healthScaleFactor>1.0</healthScaleFactor>
      <allowEmptyResults>false</allowEmptyResults>
    </hudson.tasks.junit.JUnitResultArchiver>
    <org.jenkinsci.plugins.bitbucket.BitbucketBuildStatusNotifier plugin="bitbucket-build-status-notifier@1.2.2">
      <notifyStart>true</notifyStart>
      <notifyFinish>true</notifyFinish>
      <credentialsId>jenkins_oauth_bitbucket</credentialsId>
    </org.jenkinsci.plugins.bitbucket.BitbucketBuildStatusNotifier>
    <hudson.tasks.Mailer plugin="mailer@1.19">
      <recipients>sudhakar.reddy.lakku@accenture.com</recipients>
      <dontNotifyEveryUnstableBuild>false</dontNotifyEveryUnstableBuild>
      <sendToIndividuals>true</sendToIndividuals>
    </hudson.tasks.Mailer>
  </publishers>
  <buildWrappers>
    <org.jenkinsci.plugins.credentialsbinding.impl.SecretBuildWrapper plugin="credentials-binding@1.8">
      <bindings>
        <org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
          <credentialsId>0e9cfc95-5e1c-40da-8592-d0f255f3d0a7</credentialsId>
          <usernameVariable>sf.username</usernameVariable>
          <passwordVariable>sf.password</passwordVariable>
        </org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
      </bindings>
    </org.jenkinsci.plugins.credentialsbinding.impl.SecretBuildWrapper>
  </buildWrappers>
</project>
